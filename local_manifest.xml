<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remove-project name="LineageOS/android_system_qcom"/>

  <remote name="omni" fetch="https://github.com/omnirom/"/>

<!-- bluetooth -->
  <remove-project name="LineageOS/android_system_bt"/>
  <remove-project name="LineageOS/android_packages_apps_Bluetooth"/>  
  <!--project path="packages/apps/Bluetooth" name="danielhk/android_packages_apps_Bluetooth"/-->

  <remove-project name="LineageOS/android_packages_apps_BluetoothExt"/>

<!-- browser -->
  <remove-project name="LineageOS/android_packages_apps_Browser"/>
  <remove-project name="LineageOS/android_packages_apps_Gello"/>
  <project path="packages/apps/Gello" name="LineageOS/android_packages_apps_Gello" revision="cm-14.1" />
  <!--project path="packages/apps/Browser" name="omnirom/android_packages_apps_Browser" revision="android-6.0"/-->


  <remote name="gcode" fetch="https://code.google.com/p/"/>
  <remote name="korg" fetch="git://git.kernel.org/pub/scm/bluetooth/"/>
  <remote name="bluez-android" fetch="https://github.com/bluez-android"/>

  <project path="external/bluetooth/bluez" name="danielhk/android_external_bluetooth_bluez"/>
  <project remote="korg" path="external/bluetooth/sbc" name="sbc" group="pdk" revision="master"/>
  <project remote="bluez-android" path="external/bluetooth/glib" name="glib" group="pdk" revision="master"/>

<!-- keyboard -->
  <remove-project name="LineageOS/android_packages_inputmethods_LatinIME"/>
  <project path="packages/inputmethods/LatinIME" name="platform/packages/inputmethods/LatinIME" remote="aosp"/>


<!-- device related -->
  <project path="external/stlport" name="LineageOS/android_external_stlport" />
  <project path="hardware/atheros" name="danielhk/android_hardware_atheros_wlan" />
  <project path="hardware/samsung" name="LineageOS/android_hardware_samsung" />
  <project path="packages/apps/SamsungServiceMode" name="LineageOS/android_packages_apps_SamsungServiceMode" />


<!-- device trees and kernel -->
  <project name="danielhk/android_kernel_samsung_smdk4210" path="kernel/samsung/smdk4210" />
  <project name="danielhk/proprietary_vendor_samsung_smdk4210" path="vendor/samsung" />

  <project name="danielhk/android_device_samsung_smdk4210-tab" path="device/samsung/smdk4210-tab" />
  <project name="danielhk/android_device_samsung_p6800" path="device/samsung/p6800" />
  <project name="danielhk/android_device_samsung_p6810" path="device/samsung/p6810" />
  <project name="danielhk/android_device_samsung_i815" path="device/samsung/i815" />
  <project name="danielhk/android_device_samsung_p6200" path="device/samsung/p6200" />
  <project name="danielhk/android_device_samsung_p6210" path="device/samsung/p6210" />

